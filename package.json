{
  "name": "test_totsystems",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node --experimental-json-modules app.js",
    "server": "nodemon --experimental-json-modules app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "client:heroku": "npm run client:install && npm run client:build",
    "local": "npm instal && npm run client:install",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "deploy": "git push heroku main"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fetch-headers": "^3.0.1",
    "path": "^0.12.7",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "vercel": "^25.0.1"
  }
}
